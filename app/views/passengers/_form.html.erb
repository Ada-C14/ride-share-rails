<%= form_with model: @passenger do |f| %>
  <p>Please provide the following information to <%= action_name %> passenger in our database:</p>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone_num %>
  <%= f.text_field :phone_num %>

  <%= f.submit "#{ action_name } Passenger" %>
<% end %>