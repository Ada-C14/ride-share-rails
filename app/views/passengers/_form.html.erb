<div>

<%= form_with model: @passenger do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <br>

    <div>
      <%= f.label :phone_num %>
      <%= f.text_field :phone_num %>
    </div>
    <br>

    <%= f.submit action_name, class: "button" %>

  <% end %>
</div>