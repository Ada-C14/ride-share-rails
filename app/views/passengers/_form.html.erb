<%= form_with model: @passenger, class: 'passenger-form' do |f| %>
  <p>Please fill in the form to update our database:</p>

  <p><%= f.label :name %>
    <%= f.text_field :name %></p>

  <p><%= f.label :phone_num %>
    <%= f.text_field :phone_num %></p>

  <p> <%= f.submit action_name, class: 'passenger-button' %></p>
<% end %>