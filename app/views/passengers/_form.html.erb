<div class="passenger">
  <%=form_with model: @passenger do |f|%>
    <p>Please the following information for a passenger</p>
    <div class="label">
      <%= f.label :name %> &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
      <%= f.text_field :name %>
    </div>
    <div class="label">
      <%= f.label :phone_num %>&nbsp
      <%= f.text_area :phone_num %>
    </div>
    <%= f.submit action_name %>
  <% end %>
</div>