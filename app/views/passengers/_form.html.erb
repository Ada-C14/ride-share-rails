
<%= form_with model: @passenger, class: 'create-passenger' do |f| %>
  <p>PLEASE FILL IN THE FOLLOWING PASSENGER INFORMATION:</p>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone_num %>
  <%= f.text_field :phone_num %>

  <%= f.submit "#{action_name} PASSENGER", class: "button" %>
<% end %>